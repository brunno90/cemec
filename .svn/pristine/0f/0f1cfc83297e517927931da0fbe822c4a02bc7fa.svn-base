package br.ifirst.cemec.entidades;

import java.sql.Date;
import java.util.List;

/**
 *
 * @author Iagho
 */
public class Frequentador {

    private int cdFrequentador;
    private String nmFrequentador;
    private String email;
    private Date dtCad;
    private Date dtNasc;
    private String nmMae;
    private int idade;
    private int eqCemec;
    private List<Telefone> telefones;
    private List<Tratamento> tratamentos;
    private int especial;
    private Tratamento tratamento;

    public Tratamento getTratamento() {
        return tratamento;
    }

    public void setTratamento(Tratamento tratamento) {
        this.tratamento = tratamento;
    }

    public int getEspecial() {
        return especial;
    }

    public void setEspecial(int especial) {
        this.especial = especial;
    }

    public List<Tratamento> getTratamentos() {
        return tratamentos;
    }

    public void setTratamentos(List<Tratamento> tratamentos) {
        this.tratamentos = tratamentos;
    }

    public List<Telefone> getTelefones() {
        return telefones;
    }

    public void setTelefones(List<Telefone> telefones) {
        this.telefones = telefones;
    }

    public int getEqCemec() {
        return eqCemec;
    }

    public void setEqCemec(int eqCemec) {
        this.eqCemec = eqCemec;
    }

    public int getIdade() {
        return idade;
    }

    public void setIdade(int idade) {
        this.idade = idade;
    }

    public int getCdFrequentador() {
        return cdFrequentador;
    }

    public void setCdFrequentador(int cdFrequentador) {
        this.cdFrequentador = cdFrequentador;
    }

    public String getNmFrequentador() {
        return nmFrequentador;
    }

    public void setNmFrequentador(String nmFrequentador) {
        this.nmFrequentador = nmFrequentador;
    }

    public String getEmail() {
        return email;
    }

    public void setEmail(String email) {
        this.email = email;
    }

    public Date getDtCad() {
        return dtCad;
    }

    public void setDtCad(Date dtCad) {
        this.dtCad = dtCad;
    }

    public Date getDtNasc() {
        return dtNasc;
    }

    public void setDtNasc(Date dtNasc) {
        this.dtNasc = dtNasc;
    }

    public String getNmMae() {
        return nmMae;
    }

    public void setNmMae(String nmMae) {
        this.nmMae = nmMae;
    }
}
