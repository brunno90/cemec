package br.ifirst.cemec.entidades;

import java.util.ArrayList;
import java.util.List;

/**
 *
 * @author Iagho
 */
public class ContadorSenha {

    private static List<Tratamento> contSenha;

    public static List<Tratamento> getContSenha() {
        if (contSenha == null) {
            contSenha = new ArrayList<Tratamento>();
        }
        return contSenha;
    }

    public static List<Tratamento> setAContSenha(List<Tratamento> tratamentos) {
        ContadorSenha.contSenha = tratamentos;

        return ContadorSenha.contSenha;
    }
}